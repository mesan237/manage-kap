{
  "expo": {
    "name": "manage nkap",
    "slug": "manage-nkap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "persfinanceapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "android": {
            "fonts": [
              {
                "fontFamily": "InterVar",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/InterVar-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/fonts/InterVar-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/InterVar-SemiBold.ttf",
                    "weight": 600
                  },
                  {
                    "path": "./assets/fonts/InterVar-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/InterVar-Bold.ttf",
              "./assets/fonts/InterVar-Regular.ttf",
              "./assets/fonts/InterVar-Medium.ttf",
              "./assets/fonts/InterVar-SemiBold.ttf"
            ]
          }
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.managenkap"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8e4e089d-c058-4b42-b4fd-0f47f2998aec"
      }
    },
    "owner": "mesan237"
  }
}
